{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "baseUrl": "./",
    "outDir": "build",
    "module": "esnext",
    "target": "es5",
    "lib": ["es6", "dom"],
    "allowJs": true,
    "jsx": "react",
    "moduleResolution": "node",
    "rootDir": "./src",
    "forceConsistentCasingInFileNames": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "suppressImplicitAnyIndexErrors": true,
    "noUnusedLocals": true,
    "paths": {
      "build/*": ["./build/*"],
      "server/*": ["./server/*"],
      "src/*": ["./src/*"],
      "media/*": ["./src/media/*"],
      "modules/*": ["./src/modules/*"]
    },
  },
  "exclude": [
    "node_modules",
    "build",
    "src/registerServiceWorker.ts"
  ]
}